<Shell
    x:Class="AgendaPersonal.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AgendaPersonal"
    Shell.FlyoutBehavior="Flyout">

    <FlyoutItem Title="Inicio" Icon="icon_home.png">
        <ShellContent Title="Inicio" ContentTemplate="{DataTemplate local:MainPage}" Route="home" />
    </FlyoutItem>

   

    <FlyoutItem Title="Configuración" Icon="icon_settings.png">
        <ShellContent Title="Configuración" ContentTemplate="{DataTemplate local:ConfiguracionPage}" />
    </FlyoutItem>



</Shell>
